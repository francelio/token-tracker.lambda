{
    "version": "2.0.0",
    "tasks": [
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/token.lambda.Tests/token.lambda.Tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "publish",
            "command": "dotnet",
            "type": "process",
            "args": [
                "publish",
                "${workspaceFolder}/token.lambda.Tests/token.lambda.Tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "watch",
            "command": "dotnet",
            "type": "process",
            "args": [
                "watch",
                "run",
                "${workspaceFolder}/token.lambda.Tests/token.lambda.Tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "build",
            "command": "dotnet",
            "type": "process",
            "args": [
                "build",
                "${workspaceFolder}/token.lambda.Tests/token.lambda.Tests.csproj"
            ],
            "problemMatcher": "$tsc"
        },
        {
            "label": "deploy",
            "command": "dotnet",
            "type": "process",
            "args": [
                "lambda",
                "deploy-function",
                "token",
                "--region",
                "us-east-1",
                "--profile",
                "TokenLambda",
                "--function-role",
                "LambdaService"
            ],
            "options": {
                "cwd": "${workspaceFolder}/token.lambda"
            },
            "problemMatcher": []
        },
        {
            "label": "invoke",
            "command": "dotnet",
            "type": "process",
            "args": [
                "lambda",
                "invoke-function",
                "token",
                "--region",
                "us-east-1",
                "--profile",
                "TokenLambda",
                "--payload",
                "Just Checking If Everything is OK"
            ],
            "problemMatcher": []
        }
    ]
}